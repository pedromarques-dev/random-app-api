{
    "version": 2,
    "rewrites": [{ "source": "/(.*)", "destination": "/" }],
     "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.)",
        "dest": "app.js",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": ""
        }
      }
    ]
  }